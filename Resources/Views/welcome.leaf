#extend("base")

#export("head") {
	
	<script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
	<script src="https://cdn.rawgit.com/xremix/xGallerify/master/dist/jquery.xgallerify.min.js"></script>
	
}

#export("body") {

  <div class="photos">
		#loop(pugs, "pug") {
    	<div class="photo">
      	<img src="#(pug.imageurl)">
      </div>
		}
  </div>
    
	<script>
	$('.photos').gallerify({
		margin: 0,
		mode: 'default'
	});
	</script>
}
